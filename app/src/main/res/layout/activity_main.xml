        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.429"
        app:srcCompat="@drawable/goblin_hair4" />

    <ImageView
        android:id="@+id/goblin_hair5"
        android:layout_width="226dp"
        android:layout_height="440dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/ground"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.429"
        app:srcCompat="@drawable/goblin_hair5" />

    <ImageView
        android:id="@+id/goblin_click_hair1"
        android:layout_width="321dp"
        android:layout_height="457dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.386"
        app:srcCompat="@drawable/goblin_click_hair1" />

    <ImageView
        android:id="@+id/goblin_click_hair2"
        android:layout_width="321dp"
        android:layout_height="457dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.386"
        app:srcCompat="@drawable/goblin_click_hair2" />

    <ImageView
        android:id="@+id/goblin_click_hair3"
        android:layout_width="321dp"
        android:layout_height="457dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.386"
        app:srcCompat="@drawable/goblin_click_hair3" />

    <ImageView
        android:id="@+id/goblin_click_hair4"
        android:layout_width="321dp"
        android:layout_height="457dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/ground"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.386"
        app:srcCompat="@drawable/goblin_click_hair4" />

    <ImageView
        android:id="@+id/goblin_click_hair5"
        android:layout_width="321dp"
        android:layout_height="457dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.386"
        app:srcCompat="@drawable/goblin_click_hair5" />

</androidx.constraintlayout.widget.ConstraintLayout>